FoamFile
{
    version     2.2;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      H;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions    [ 0 2 -2 0 0 0 0 ];

internalField   uniform -17992.9;

boundaryField
{
    inletfuel           
    {
        type          fixedValue;
        value         uniform -2.19608e+06;
    }

    inletair           
    {
        type          fixedValue;
        value         uniform -17992.9;
    }

    outlet          
    {
        type          zeroGradient;
    }

    sidewall       
    {
        type          zeroGradient;
    }

    burnerwall
    {
        type          zeroGradient;
    }

    front
    {
        type            wedge;
    }

    back
    {
        type            wedge;
    }

    axis
    {
        type            empty;
    }
}


